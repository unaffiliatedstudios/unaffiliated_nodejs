{
  "name": "unaffiliated_nodejs",
  "version": "1.0.0",
  "description": "A NodeJS and Express instance of Unaffiliated Studios' website",
  "main": "index.js",
  "scripts": {
    "coverage": "npm run pretest && jest --coverage && cat ./coverage/lcov.info | coveralls",
    "dev": "nodemon ./index.js",
    "ui": "browser-sync start --proxy=localhost:8000 --files='**/*.css, **/*.pug, **/*.js' --ignore=node_modules --reload-delay 10 --no-ui --no-notify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unaffiliatedstudios/unaffiliated_nodejs.git"
  },
  "keywords": [
    "NodeJS",
    "Express",
    "unaffiliatedstudios"
  ],
  "author": "Cedric Johnson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/unaffiliatedstudios/unaffiliated_nodejs/issues"
  },
  "homepage": "https://github.com/unaffiliatedstudios/unaffiliated_nodejs#readme",
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "coveralls": "^3.1.0",
    "nodemon": "^2.0.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pug": "^3.0.0"
  }
}
